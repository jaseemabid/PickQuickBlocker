{
		"name": "PickQuick Blocker",
		"version": "1.0",
		"manifest_version": 1,
		"description": "Making the Internet better!",
		"permissions": [
				"http://*.facebook.com/*",
				"https://*.facebook.com/*",
				"http://facebook.com/*",
				"https://facebook.com/*"
		],
		"content_scripts": [
				{
						"matches": [
								"http://*.facebook.com/*",
								"https://*.facebook.com/*",
								"http://facebook.com/*",
								"https://facebook.com/*"
						],
						"js": [
								"jquery-1.7.2.min.js",
								"underscore-min.js",
								"pickquick.js"
						]
				}
		]
}
